{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "extra_command",
    "version": "${version}",
    "metadata": {
      "name": "extra command",
      "description": "Add more commands to Minecraft!",
      "contributors": {
        "hanana": "Author"
      },
      "icon": "assets/extra_command/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "org.eu.hanana.reimu.mc.extra_cmd.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.21"
      },
      {
        "id": "architectury",
        "version": ">=13.0.2"
      }
    ]
  },
  "mixin": [
    "extra_command.mixins.json"
  ]
}
